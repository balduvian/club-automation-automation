{
  "manifest_version": 2,
  "name": "Club Automation Automation",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://ic.clubautomation.com/*"
      ],
      "js": ["content.js"]
    }
    ],
    "browser_action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_title": "Club Auto Auto",      
      "default_popup": "options.html"
    },
    "permissions": [
      "storage"
    ]
}